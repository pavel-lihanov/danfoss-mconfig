<table>
{% for key,value in package.attributes.items %}
	<tr>
		<td>
			{{ key }}
		</td>
		<td>
			{{ value }}
		</td>		
	</tr>
{% endfor %}
</table>
<table>
{% for key,value in package.options.items %}
